    <script>
        function show () {
            document.getElementById('dataForm').style.display='block';
        };
        function print1() {
            // Truy cập đối tượng select
            var selectElement = document.getElementById("mySelect1");
            // Truy cập phần tử được chọn
            var selectedOption = selectElement.options[selectElement.selectedIndex];

            // Lấy giá trị và nội dung của phần tử được chọn
            var selectedValue = selectedOption.value;
            var selectedText = selectedOption.text;

            // In thông tin phần tử được chọn
            var selectedOptgroup = selectedOption.parentNode;
            var optgroupName = selectedOptgroup.label;
            // In thông tin phần tử được chọn
            if (selectedText != "--Chọn xe--") {
                document.getElementById("chonxe1").innerHTML = optgroupName + " " + selectedText + '<br>';
                document.getElementById("chonxe1").style.backgroundColor = "white";
                document.getElementById("chonxe1").style.color = "black";
                document.getElementById("chonxe1").style.height = "200px";
                document.getElementById("chonxe1").style.width = "10vw";
                var img = document.createElement("img");
                img.src = "https://i.pinimg.com/564x/96/56/17/965617885d66d7d688ee0b24e3aa7892.jpg"; // Đường dẫn đến hình ảnh của bạn
                img.alt = "My Image";
                img.style.width = "100px"; // Thiết lập chiều rộng và chiều cao của hình ảnh
                img.style.height = "100px";
                document.getElementById("chonxe1").appendChild(img); // Thêm hình ảnh vào nút
                document.getElementById("tt2").style.height = "200px";
                document.getElementById("tt2").style.width = "10vw";
                document.getElementById("tt3").style.height = "200px";
                document.getElementById("tt3").style.width = "10vw"; 
                document.getElementById("tt4").style.height = "200px";
                document.getElementById("tt4").style.width = "10vw";
            }

            else {
                document.getElementById("chonxe1").innerHTML = 'Chọn xe';
                var alertMessageDiv = document.getElementById('alertMessage');
                alertMessageDiv.classList.add('alert', 'alert-danger');
                alertMessageDiv.textContent = 'Bạn hãy chọn một chiếc xe muốn so sánh';

                $('#alertModal').modal('show');
            }
            selectedOption.value = selectedValue;
            selectedOption.text = selectedText;
        }
        function print2() {
